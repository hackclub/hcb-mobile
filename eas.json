{
  "cli": {
    "version": ">= 16.19.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "channel": "development",
      "env": {
        "EXPO_USE_SUBMODULES": "1",
        "GRADLE_OPTS": "-Xmx4g -XX:MaxMetaspaceSize=4g",
        "JAVA_OPTS": "-Xmx4g"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "env": {
        "EXPO_USE_SUBMODULES": "1",
        "GRADLE_OPTS": "-Xmx4g -XX:MaxMetaspaceSize=4g",
        "JAVA_OPTS": "-Xmx4g"
      }
    },
    "production": {
      "channel": "production",
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}
